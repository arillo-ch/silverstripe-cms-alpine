{
    "name": "arillo/silverstripe-cms-alpine",
    "type": "silverstripe-vendormodule",
    "description": "Makes alpinejs available in silverstripe CMS",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "alpinejs"
    ],
    "authors": [
        {
            "name": "Arillo Team",
            "email": "hello@arillo.ch"
        }
    ],
    "homepage": "https://github.com/arillo-ch/silverstripe-cms-alpine",
    "support": {
        "issues": "https://github.com/arillo-ch/silverstripe-cms-alpine/issues"
    },
    "require": {
        "silverstripe/vendor-plugin": "^2.0 || ^3.0",
        "silverstripe/admin": "^3.0 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "Arillo\\Alpine\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
